# import practice

import os
import time

def type_effect(text, delay = 0.03):
    for char in text:
        print(char, end="", flush=True)
        time.sleep(delay)
    print()

type_effect("hello bro, are you ready to become an ai engineer?", 0.05)
time.sleep(1)
os.system('cls' if os.name == 'nt' else 'clear')
type_effect("learning logic mode activated...", 0.05)